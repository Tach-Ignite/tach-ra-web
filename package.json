{
  "name": "tach-ra-web",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "**/mappingProfile.ts"
  ],
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node --skip-project server.ts",
    "build": "next build",
    "start": "next start",
    "test": "jest --config jest.config.mjs",
    "test:watch": "jest --watch --config jest.config.mjs",
    "test:coverage": "jest --coverage --config jest.config.mjs",
    "data:seed": "tsx --tsconfig src/lib/utils/data/seeder/tsconfig.runSeeder.json src/lib/utils/data/seeder/runSeeder.ts",
    "data:seed:dev": "tsx --tsconfig src/lib/utils/data/seeder/tsconfig.runSeeder.json src/lib/utils/data/seeder/runSeeder.ts  --env dev",
    "lint": "next lint --no-cache --fix",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install",
    "pretty": "prettier --ignore-path .prettierignore --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "pnpm pretty && pnpm lint --fix",
    "setup-git-aws-connectivity": "cd infra/sst/apps/githubAwsConnectivitySetup && npx sst deploy",
    "deploy-env-vars": "tsx --tsconfig ./tsconfig.json ./deployEnvVars.ts",
    "deploy-app": "npx sst deploy",
    "remove-app": "npx sst remove"
  },
  "packageManager": "pnpm@8.6.9",
  "pnpm": {
    "patchedDependencies": {
      "next@13.4.12": "patches/next@13.4.12.patch"
    },
    "overrides": {
      "aws-cdk-lib>constructs": "10.3.0",
      "sst>constructs": "10.3.0",
      "esbuild": "0.17.19",
      "next-swagger-doc>swagger-jsdoc": "npm:@jersmart/swagger-jsdoc@^6.1.1"
    }
  },
  "dependencies": {
    "@auth/core": "^0.8.4",
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/client-ses": "^3.478.0",
    "@aws-sdk/client-sns": "^3.478.0",
    "@aws-sdk/client-sqs": "^3.478.0",
    "@aws-sdk/client-ssm": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@hookform/resolvers": "^3.1.1",
    "@jersmart/automapper-core": "^0.0.0",
    "@jersmart/automapper-pojos": "^0.0.0",
    "@next-auth/mongodb-adapter": "^1.1.3",
    "@paypal/react-paypal-js": "^8.1.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@serdnam/pino-cloudwatch-transport": "^1.0.4",
    "@stripe/stripe-js": "^2.0.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/react-redux": "^7.1.25",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "autoprefixer": "10.4.14",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "cookies": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "exports-loader": "^4.0.0",
