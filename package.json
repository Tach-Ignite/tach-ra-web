    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^5.6.0",
    "mongoose": "^7.3.1",
    "next": "13.4.12",
    "next-auth": "^4.22.1",
    "next-connect": "^1.0.0",
    "next-recaptcha-v3": "^1.2.0",
    "next-swagger-doc": "^0.4.0",
    "next-themes": "^0.2.1",
    "pino": "^8.15.0",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-datepicker": "^4.14.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.1",
    "react-responsive-carousel": "^3.2.23",
    "react-spinners": "^0.13.8",
